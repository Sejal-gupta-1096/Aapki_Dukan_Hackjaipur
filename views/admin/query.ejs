<div class="content pb-0">
    <div class="orders">
       <div class="row">
          <div class="col-xl-12">
             <div class="card">
                <div class="card-body">
                   <h3><%= user.name %> </h3>
                </div>
                <div class="card-body--">
                   <div class="table-stats order-table ov-h">
                      <table class="table ">
                         <thead>
                            <tr>
                               <th class="serial">Serial</th>
                               <th>Title</th>
                               <th>Message</th>
                               <th></th>
                            </tr>
                         </thead>
                         <tbody>
                            <% for(let i = 0 ; i < user.query.length ; i++){ %>
                                 <tr>
                                    <td class="serial"><%= i+1 %></td>
                                    <td> <span class="name"><%= user.query[i].title %></span> </td>
                                    <td> <span class="name"><%= user.query[i].message %></span> </td>
                                    <td>
                                       <span class="badge badge-success"><a style="color:white;" href="/admin/master/contact_master/delete?id=<%= user.query[i]._id %>">Resolved</a></span>
                                    </td>
                                   </tr>
                             <% } %>
                         </tbody>
                      </table>
                   </div>
                </div>
             </div>
          </div>
       </div>
    </div>
  </div>
 <div class="clearfix"></div>
</div>

