</head>

<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->  

    <!-- Body main wrapper start -->
    <div class="wrapper">
        <!-- Start Header Style -->
        <header id="htc__header" class="htc__header__area header--one">
            <!-- Start Mainmenu Area -->
            <div id="sticky-header-with-topbar" class="mainmenu__wrap sticky__header">
                <div class="container">
                    <div class="row">
                        <div class="menumenu__container clearfix">
                            <div class="col-lg-2 col-md-2 col-sm-3 col-xs-5"> 
                                <div class="logo">
                                     <a href="/"><img src="/website/images/4.png" alt="logo images"></a>
                                </div>
                            </div>
                            <div class="col-md-7 col-lg-8 col-sm-5 col-xs-3">
                                <nav class="main__menu__nav hidden-xs hidden-sm">
                                    <ul class="main__menu">
                                        <li class="drop"><a href="/">Home</a></li>
                                        <li class="drop"><a href="#">CATEGORY</a>
                                            <ul class="dropdown">
                                                <% for(let i = 0 ; i < categories_list.length ; i++){ %>
                                                    <% if(categories_list[i].status == 1) {%>
                                                        <li><a href="/category?id=<%= categories_list[i]._id %>"><%= categories_list[i].name %></a></li>
                                                    <% } %>
                                                    
                                                <% } %>
                                                
                                            </ul>
                                        </li>
                                        <li class="drop"><a href="/about-us">ABOUT US</a>
                                        <li><a href="/contact-us">contact</a></li>
                                       
                                    </ul>
                                </nav>

                                <div class="mobile-menu clearfix visible-xs visible-sm">
                                    <nav id="mobile_dropdown">
                                        <ul>
                                            <li><a href="/">Home</a></li>
                                            <% for(let i = 0 ; i < categories_list.length ; i++){ %>
                                                <% if(categories_list[i].status == 1) {%>
                                                    <li><a href="/category/?id=<%=categories_list[i]._id%>"><%= categories_list[i].name %></a></li>
                                                <% } %>
                                                
                                            <% } %>
                                            <li class="drop"><a href="/about-us">ABOUT US</a>
                                            <li><a href="/contact-us">contact</a></li>
                                            
                                        </ul>
                                    </nav>
                                </div>  
                            </div>
                            <div class="col-md-3 col-lg-2 col-sm-4 col-xs-4">
                                <div class="header__right">
                                    <% if(locals.user){%>
                                        <div class="header__account">
                                            <a href="/user/account/"><i class="icon-user icons"></i></a>
                                        </div>
                                        <h3 style="margin-right: 1rem;justify-content: center;"><a href="/user/logout">LogOut</a></h3>
                                    <% }else{ %>
                                        <h3 style="margin-right: 1rem;justify-content: center;"><a href="/login-register">SignUp</a></h3>
                                    <% } %>
                                    
                                    <% if(locals.user) { %>
                                        <div class="htc__shopping__cart">
                                            <a class="cart__menu" href="#"><i class="icon-handbag icons"></i></a>
                                            <a href="#"><span class="htc__qua"><%= locals.user.cart.length %></span></a>
                                        </div>
                                    <% } %>


                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="mobile-menu-area"></div>
                </div>
            </div>
            <!-- End Mainmenu Area -->
        </header>
        <!-- End Header Area -->
         <!-- Start Cart Panel -->
         <% if(locals.user) {%>
            <div class="shopping__cart">
                <div class="shopping__cart__inner">
                    <div class="offsetmenu__close__btn">
                        <a href="#"><i class="zmdi zmdi-close"></i></a>
                    </div>
                    <div class="shp__cart__wrap">
                        <% for(let i = 0 ; i < whole_user.cart.length ; i++){ %>
                            <div class="shp__single__product">
                                <div class="shp__pro__thumb">
                                    <a href="#">
                                        <img src="<%=whole_user.cart[i].product.product_img%>" alt="product images">
                                    </a>
                                </div>
                                <div class="shp__pro__details">
                                    <h2><a href=""><%=whole_user.cart[i].product.name%></a></h2>
                                    <span class="quantity">QTY:<span class="qty" ><%=whole_user.cart[i].qty%></span> </span>
                                    <span class="shp__price">₹<span class="amt"><%=whole_user.cart[i].product.sp%></span></span>
                                </div>
                                <div class="remove__btn">
                                    <a href="/user/cart/delete/?id=<%=whole_user.cart[i]._id%>" title="Remove this item"><i class="zmdi zmdi-close"></i></a>
                                </div>
                            </div>
                        <% } %>
                        
                    </div>
                    <ul class="shoping__total">
                        <li class="subtotal">Subtotal:</li>
                        <li class="total__price">₹</li>
                    </ul>
                    <ul class="shopping__btn">
                        <li><a href="/user/cart/view">View Cart</a></li>
                        <li class="shp__checkout"><a href=
                            "/user/cart/checkout">Checkout</a></li>
                    </ul>
                </div>
            </div>
        <% } %>
        
        <!-- End Cart Panel -->
       
        
            <%- body %>
       
            <footer id="htc__footer">
                <!-- Start Footer Widget -->
                <div class="footer__container bg__cat--1">
                    <div class="container">
                        <div class="row">
                            <!-- Start Single Footer Widget -->
                            <div class="col-md-3 col-sm-6 col-xs-12">
                                <div class="footer">
                                    <h2 class="title__line--2">ABOUT US</h2>
                                    <div class="ft__details">
                                        <p>Aapki Dukaan encourages all the small scale enterpeneurs to come online and expand their business. Soon we will be available on all social media platforms. </p>
                                        <div class="ft__social__link">
                                            <ul class="social__link">
                                                <li><a href="#"><i class="icon-social-twitter icons"></i></a></li>
    
                                                <li><a href="#"><i class="icon-social-instagram icons"></i></a></li>
    
                                                <li><a href="#"><i class="icon-social-facebook icons"></i></a></li>
    
                                                <li><a href="#"><i class="icon-social-google icons"></i></a></li>
    
                                                <li><a href="#"><i class="icon-social-linkedin icons"></i></a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- End Single Footer Widget -->
                            <!-- Start Single Footer Widget -->
                            <div class="col-md-2 col-sm-6 col-xs-12 xmt-40">
                                <div class="footer">
                                    <h2 class="title__line--2">information</h2>
                                    <div class="ft__inner">
                                        <ul class="ft__list">
                                            <li><a href="/about-us">About us</a></li>
                                            <li><a href="/about-us ">Terms  & Condition</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!-- End Single Footer Widget -->
                       
                            <!-- Start Single Footer Widget -->
                            <div class="col-md-2 col-sm-6 col-xs-12 xmt-40 smt-40">
                                <div class="footer">
                                    <h2 class="title__line--2">Our service</h2>
                                    <div class="ft__inner">
                                        <ul class="ft__list">
                                            <li><a href="/user/account/ ">My Account</a></li>
                                            <li><a href="/user/cart/view">My Cart</a></li>
                                            <li><a href="/user/login-register ">Login</a></li>
                                            <li><a href="/user/cart/checkout">Checkout</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!-- End Single Footer Widget -->
                            <!-- Start Single Footer Widget -->
                            <div class="col-md-3 col-sm-6 col-xs-12 xmt-40 smt-40">
                                <div class="footer">
                                    <h2 class="title__line--2">NEWSLETTER </h2>
                                    <div class="ft__inner">
                                        <div class="news__input">
                                            <input type="text" placeholder="Your Mail*">
                                            <div class="send__btn">
                                                <a class="fr__btn" href="#">Send Mail</a>
                                            </div>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                            <!-- End Single Footer Widget -->
                        </div>
                    </div>
                </div>
                <!-- End Footer Widget -->
                <!-- Start Copyright Area -->
                <div class="htc__copyright bg__cat--5">
                    <div class="container">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="copyright__inner">
                                    <p>Copyright© <a href="#">Socio-preneurs</a> 2020. All rights reserved.</p>
                                    <a href="#"><img src="/website/images/paypol.png" alt="payment images"></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End Copyright Area -->
            </footer>
            <!-- End Footer Style -->
        </div>
        <!-- Body main wrapper end -->
    
        <!-- Placed js at the end of the document so the pages load faster -->
    
        <script src="https://cdnjs.cloudflare.com/ajax/libs/noty/3.1.4/noty.min.js"></script>
        <script>
            <% if(flash.success && flash.success.length > 0) { %>
            new Noty({
                layout: 'topRight',
                theme: 'relax', // or relax
                type: 'success', // success, error, warning, information, notification
                text: '<%= flash.success %>', // [string|html] can be HTML or STRING
                timeout : 1500
            }).show();
        <% } %>
        <% if(flash.error && flash.error.length > 0) { %>
            new Noty({
                layout: 'topRight',
                theme: 'relax', // or relax
                type: 'error', // success, error, warning, information, notification
                text: '<%= flash.error %>', // [string|html] can be HTML or STRING
                timeout : 1500
            }).show();
        <% } %>
        </script>
        <!-- jquery latest version -->
        <script src="/website/js/vendor/jquery-3.2.1.min.js"></script>
        <!-- Bootstrap framework js -->
        <script src="/website/js/bootstrap.min.js"></script>
        <!-- All js plugins included in this file. -->
        <script src="/website/js/plugins.js"></script>
        <script src="/website/js/slick.min.js"></script>
        <script src="/website/js/owl.carousel.min.js"></script>
        <!-- Waypoints.min.js. -->
        <script src="/website/js/waypoints.min.js"></script>
        <!-- Main js file that contents all jQuery plugins activation. -->
        <script src="/website/js/main.js"></script>
        <script src="/website/js/math_operations.js"></script>
        <%- script %>

    </body>

    </html>